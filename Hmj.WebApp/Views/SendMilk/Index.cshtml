                var image = new Image();
                image.src = $("img", $(this)).attr("src");
                var canvas = document.createElement("canvas");
                var ctx = canvas.getContext('2d');
                canvas.width = '720';
                canvas.height = '1113';

                var conts = $(this).attr("exstr");
                var fid = $(this).attr("fid");
                $("#number").html(conts);

                $("#filseid").val(fid);

                image.onload = function () {
                    ctx.drawImage(this, 0, 0, 720, 1160, 0, 0, this.width, 1113);
                    $('.p4-img').attr('src', canvas.toDataURL());
                    $('.page4').show();
                }

            });
            $('.page4 a.close').click(function () {
                $('.page4').hide();

            })

            $('.page4 .heart>img').click(function () {
                //粉丝点赞在这里异步调用
                var fansid = $("#openid").val();
                var fid = $("#filseid").val();
                $.ajax({
                    type: "post",
                    url: "/SendMilk/Good.do",
                    data: { FansID: fansid, FileID: fid },
                    success: function (data) {
                        testAnim();
                        $('.page4 .heart span').text(parseInt($('.page4 .heart span').text()) + 1);
                    }
                })
            })
        })

        function testAnim() {
            $('.page4 .heart>i').removeClass().addClass('fadeInUp').one('webkitAnimationEnd mozAnimationEnd MSAnimationEnd oanimationend animationend', function () {
                $(this).removeClass();
            });
        };
        function getPra(n) {
            n = n + '';
            var arr = [],
                len = n.length,
                i = 0;
            for (i; i < len; i++) {
                arr.push(parseInt(n.substr(i, 1)));
            }
            return arr;
        }

        function setdate() {
            var year = mydata.getFullYear();
            var year1 = getPra(year);
            $('.timebox .date .year span:nth-child(1)').attr('class', 'num' + year1[0]);
            $('.timebox .date .year span:nth-child(2)').attr('class', 'num' + year1[1]);
            $('.timebox .date .year span:nth-child(3)').attr('class', 'num' + year1[2]);
            $('.timebox .date .year span:nth-child(4)').attr('class', 'num' + year1[3]);
            var month = (mydata.getMonth() + 1).toString().length > 1 ? parseInt(mydata.getMonth() + 1) : ('0' + parseInt(mydata.getMonth() + 1));
            var month1 = getPra(month);
            $('.timebox .date .month span:nth-child(1)').attr('class', 'num' + month1[0]);
            $('.timebox .date .month span:nth-child(2)').attr('class', 'num' + month1[1]);
            var day = mydata.getDate().toString().length > 1 ? parseInt(mydata.getDate()) : ('0' + parseInt(mydata.getDate()));
            var day1 = getPra(day);
            $('.timebox .date .day span:nth-child(1)').attr('class', 'num' + day1[0]);
            $('.timebox .date .day span:nth-child(2)').attr('class', 'num' + day1[1]);
            var hour = mydata.getHours().toString().length > 1 ? parseInt(mydata.getHours()) : ('0' + parseInt(mydata.getHours()));
            var hour1 = getPra(hour);
            $('.timebox .time .hour span:nth-child(1)').attr('class', 'num' + hour1[0]);
            $('.timebox .time .hour span:nth-child(2)').attr('class', 'num' + hour1[1]);
            var mins = mydata.getMinutes().toString().length > 1 ? parseInt(mydata.getMinutes()) : ('0' + parseInt(mydata.getMinutes()));
            var mins1 = getPra(mins);
            $('.timebox .time .mins span:nth-child(1)').attr('class', 'num' + mins1[0]);
            $('.timebox .time .mins span:nth-child(2)').attr('class', 'num' + mins1[1]);
            /*设置首页文字*/
            var houer2 = parseInt(mydata.getHours()) + 1;

            $('.changepictxt>div.txt' + houer2 + '.hide').removeClass('hide').siblings('div').addClass('hide');

        }
    </script>
