        var URLDOMAIN = "http://vipclub.lining.com/pengyouquan/";
        var APIDOMAIN = "http://tt.puman.cn/milk/index.php";

        document.querySelector('body').addEventListener('touchmove', function (e) {
            if (!document.querySelector('.scroll-elem').contains(e.target)) {
                e.preventDefault();
            }
        })

        window.onload = function () {
            var music_key = 0;
            var openMusic = function () {
                var self = this;
                self.t = "";
                self.n = 0;
                self.allNumber = 29;
                self.v = 15;

                self.begin = function () {
                    clearInterval(self.t);
                    self.t = setInterval(function () {
                        var x = Math.floor(self.n / self.v) * -1 * 128 + "px";
                        var y = self.n % self.v * -1 * 128 + "px";
                        $("#music").css("background-position", y + " " + x)

                        self.n++;
                        if (self.n >= self.allNumber - 1) {
                            self.n = 0;
                        }
                    },
                        80);
                }

                self.end = function () {
                    self.n = 0;
                    $("#music").css("background-position", "0px 0px");
                    clearInterval(self.t);
                }
            }

            audioElement2 = document.getElementById("call_audio");
            window.musicKey = new openMusic();

            audioElement2.play();
            musicKey.begin();
            music_key = 1;

            setTimeout(function () {
                $(window).scrollTop(1);
            }, 10);

            document.addEventListener("WeixinJSBridgeReady", function () {
                WeixinJSBridge.invoke('getNetworkType', {}, function (e) {
                    document.getElementById("call_audio").play();
                    musicKey.begin();
                    music_key = 1;
                });
            }, false);

            $("#music").on("touchstart", function () {
                if (music_key == 1) {
                    audioElement2.pause();
                    music_key = 0;
                    musicKey.end();
                } else {
                    audioElement2.play();
                    music_key = 1;
                    musicKey.begin();
                }
                return false;

            });

        }
    </script>
    <script type="text/javascript">
        /*获取时间*/
        var mydata = new Date();

        $(function () {

            $('.pictxt .rdtxt').arctext({
                radius: 300
            });
            setdate();
            var timer = setInterval(function () {
                setdate();
            }, 1000);
            $('.piclist a').click(function () {

